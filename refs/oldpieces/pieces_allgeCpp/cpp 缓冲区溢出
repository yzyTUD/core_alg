#include <stdio.h>
#include <string.h>
char pa[10]; int flag = 0; char pass[10];


int main(){
    while (1){
    puts("plese input your password\n");
    gets(pass);
    strcpy(pa, pass);
    flag = 0;
    if (strcmp(pa, "1234") == 0)
        flag = 1;
    if (flag)
        puts("correct!");
    else
        puts("wrong answer!");
    }
    return 0;
}
