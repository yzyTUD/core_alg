#include <iostream>
typedef unsigned int uint;

class Box
{
    int num;

  public:
    int getNum()
    {
        return num;
    }
    void setNum(int n)
    {
        num = n;
    }
};

int main()
{
    Box a;
    a.setNum(12);
    std::cout << "the number we set = " << a.getNum() << std::endl;

    return 0;
}
