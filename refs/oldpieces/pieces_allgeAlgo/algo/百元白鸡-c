#include <stdio.h>
//100   100ji   
//5  3  1/3
int main(){
     int a,b,c;
     for(a=0;a<=20;a++)
         for(b=0;b<=33;b++)
             for(c=3;c<=99;c++)
                 if(a*5+b*3+c/3==100)
                     if(a+b+c==100)
                         if(c%3==0)
                             printf("cock:%d hen:%d chick:%d\n",a,b,c);

     return 0;
}
