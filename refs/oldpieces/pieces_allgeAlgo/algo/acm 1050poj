/*
in:

4
0 -2 -7 0 9 2 -6 2
-4 1 -4  1 -1

8  0 -2

out:
15

*/

#include <iostream>
using namespace std;

int main()
{

    int N;
    cin >> N;
    int A[N][N];
    for (int i = 0; i < N; i++)
    {
        for (int j = 0; j < N; j++)
        {
            cin >> A[i][j];
        }
    }

    int global_max = 0;
    for (int i = 0; i < N; i++)
    {
        for (int j = 0; j < N; j++)
        {
            for (int p = 0; p < N - i; p++)
            {
                for (int q = 0; q < N - j; q++)
                {
                    int local_sum = 0;
                    for (int m = i; m < N - p + 1; m++)
                    {
                        for (int n = j; n < N - q + 1; n++)
                        {
                            local_sum += A[m][n];
                        }
                    }
                    if (global_max < local_sum)
                    {
                        global_max = local_sum;
                    }
                }
            }
        }
    }

    cout << global_max;

    return 0;
}