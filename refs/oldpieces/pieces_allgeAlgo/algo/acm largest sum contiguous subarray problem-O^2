/*
in:

7
1 3  -2  4  -5 2 1 -2

9
-2 1 -3 4 -1 2 1 -5 4
out::6


6
-2 11 -4 13 -5 -2
out ::20

*/

#include <iostream>
using namespace std;

int main()
{

    int N;
    cin >> N;
    int A[N];
    for (int i = 0; i < N; i++)
    {
        cin >> A[i];
    }

    int global_max = -100;
    for (int i = 0; i < N; i++)
    {
        for (int p = 0; p < N - i; p++)
        {
            int local_sum = 0;
            for (int m = i; m < N - p; m++)
            {
                local_sum += A[m];
            }
            if (global_max < local_sum)
            {
                global_max = local_sum;
            }
        }
    }

    cout << global_max << endl;

    return 0;
}