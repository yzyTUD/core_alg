/*前40个数*/
#include "stdio.h"
void mazePrint(long* S);
int main(){
	long f1=1, f2=2 , i=0;
	long S[100];
	while (i < 40){
		S[i] = f1;
		S[i + 1] = f2;
		i += 2;
		//更新
		f1 = f1 + f2;
		f2 = f1 + f2;
	}
	mazePrint(S);
}

void mazePrint(long* S){
	int i = 0;
	while (i<40){
		printf("%20ld", S[i]);
		i++;
		if (i % 4 == 0)
			printf("\n");
	}
}

-------------------------------------------------------------


class Main{

      public static void main(String[] args) {
            int a=1,b=1,c=1,d;
            System.out.print(a+" "+b+" "+c+" ");
            for(int i=0;i<10;i++){
                  d=a+b+c;
                  System.out.print(d+" ");
                  a=b;
                  b=c;
                  c=d;
            }
      }
}

