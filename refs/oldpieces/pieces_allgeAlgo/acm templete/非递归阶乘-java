
importjava.util.Stack;
public class StackTest {

     static int f(int x){
            Stack s =new Stack();

            while( x>1){
                 s.push(x--);
           }
            int sum=1;
            while(! s.isEmpty()){
                 sum*=(Integer) s.pop();
           }

            return sum;
     }


     public static void main(String[] args) {
           System. out.println( f(5));
     }
}

