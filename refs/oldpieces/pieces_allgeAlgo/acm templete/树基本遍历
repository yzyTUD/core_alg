#include <stdio.h>
#include <malloc.h>
typedef char NodeValue;
char str[] = "ab##c##";
int ptr = 0;
typedef struct tnode {
    NodeValue val;
    struct tnode * lchild;
    struct tnode * rchild;
}* Tree;

void CreatTree(Tree & t){
    char ch;
    ch = str[ptr++];
    if (ch == '#'){
        t = NULL;
    }
    else{
        t = (Tree)malloc(sizeof(Tree));
        t->val = ch;
        CreatTree(t->lchild);
        CreatTree(t->rchild);
    }
}

void PreOrder(Tree  t){
    if (t)
    {
        printf("%c", t->val);
        PreOrder(t->lchild);
        PreOrder(t->rchild);
    }
}

void InOrder(Tree  t){
    if (t)
    {
        PreOrder(t->lchild);
        printf("%c", t->val);
        PreOrder(t->rchild);
    }
}

void NachOrder(Tree  t){
    if (t)
    {
        PreOrder(t->lchild);
        PreOrder(t->rchild);
        printf("%c", t->val);
    }
}

int main(){

    Tree t;
    CreatTree(t);
    PreOrder(t);
    printf("\n");
    InOrder(t);
    printf("\n");
    NachOrder(t);

    getchar();
    getchar();
    return 0;
}
