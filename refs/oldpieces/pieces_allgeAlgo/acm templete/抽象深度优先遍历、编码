#include <stdio.h>
#define N 3
int x[N];

void backtrack(int t){
	if (t > N - 1){
		printf("%d %d %d\n", x[0], x[1], x[2]);
	}
	else{
		for (int i = 0; i <= 1; i++)
		{
			x[t] = i;
			if (i == 0)
				backtrack(t + 1);
			else
				backtrack(t + 1);
		}
	}
}

int main(){
	backtrack(0);
	getchar();
	return 0;
}
