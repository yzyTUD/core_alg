# -*- coding: utf-8 -*-


import wx
class MainWindow(wx.Frame):
    #继承Frame
    def __init__(self, parent, title):
        wx.Frame.__init__(self, parent, title = title, size = (200, 100))
        self.control = wx.TextCtrl(self, style = wx.TE_MULTILINE)

app = wx.App(False)
frame = MainWindow(None, 'Small editor')
frame.Show(True)
app.MainLoop()