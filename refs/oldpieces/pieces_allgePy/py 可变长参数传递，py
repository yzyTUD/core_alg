
# 一般长元组传递=============================
def func(a):
    a[3]=1
    return 0
m=[2,1,2,4]
print m
func(m)
print m


# 可变长元组传递=============================
def func(*a):
    print a[0]

m=[4,2,3,4]
n=[2,2,3,4]
func(m,n)


# 字典传递=============================
def func(**a):
    print a

func(name='bob',age=18)